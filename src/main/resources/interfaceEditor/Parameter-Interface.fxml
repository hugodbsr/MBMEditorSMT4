<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="interfaceEditor.ParameterController">
   <children>
      <VBox prefHeight="401.0" prefWidth="600.0">
         <children>
            <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="81.0" prefWidth="600.0" text="Parameters">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <HBox prefHeight="27.0" prefWidth="600.0">
               <children>
                  <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="69.0" prefWidth="242.0" text="Default directory" textAlignment="CENTER">
                     <font>
                        <Font size="18.0" />
                     </font>
                  </Label>
                  <TextField fx:id="PathDirectory" prefHeight="27.0" prefWidth="249.0" />
                  <Button fx:id="OpenDirectory" mnemonicParsing="false" text="Open Folder" />
               </children>
            </HBox>
            <HBox alignment="BOTTOM_CENTER" prefHeight="145.0" prefWidth="600.0">
               <children>
                  <Button fx:id="ApplySettings" alignment="CENTER" mnemonicParsing="false" prefHeight="30.0" prefWidth="137.0" text="Apply Settings">
                     <font>
                        <Font size="14.0" />
                     </font>
                  </Button>
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
